{
	"compilerOptions": {
		"sourceMap": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es6",
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"outDir": "./dist"
	},
	"include": ["./*.ts", "./**/*.ts", "./**/*.js", "netlify/functions/actions/router.mts", "netlify/functions/actions/index.ts", "netlify/functions/actions/EntryModel.mts"]
}
